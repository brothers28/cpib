program progConjunctional
global
   var x:nat32;
   var y:int32;
   var z:int32;
   var b:bool
do
   x init := 4;
   y init := 4;
   z init := 0;
   if [int32] x > y = 4  then
      z := ([int32] x * y)
   else
      z := ([int32] x + y)
   endif;
   b init := [int32] x >= y;
   debugout z;
   debugout b
endprogram
